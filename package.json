{
  "name": "com.rsasesorjuridico.app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "sass": "sass --no-source-map src:src && typed-scss-modules src --exportType default ",
    "lint": "next lint --fix && next lint",
    "prebuild": "pnpm sass && pnpm lint",
    "devbuild": "pnpm prebuild && next build",
    "build": "next build",
    "dev": "URL=beta.rsasesorjuridico.com NEXTAUTH_URL=https://beta.rsasesorjuridico.com next dev",
    "start": "next start",
    "db:migrate": "prisma migrate dev --name init --create-only && prisma generate",
    "db:push": "prisma db push",
    "json-to-prisma": "json-to-prisma-schema-convertor convert --inputPath='./prisma/schema.json' --outputPath='./prisma/new/schema.prisma'"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@hookform/resolvers": "^3.3.0",
    "@mui/material": "^5.14.5",
    "@prisma/client": "^5.2.0",
    "@types/bcrypt": "^5.0.0",
    "@types/node": "20.5.0",
    "@types/react": "18.2.14",
    "@types/react-dom": "18.2.7",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^2.4.3",
    "eslint": "8.47.0",
    "eslint-config-next": "13.4.19",
    "jose": "^4.14.4",
    "json-to-prisma-schema-convertor": "^0.1.0",
    "material-symbols": "^0.11.0",
    "mongodb": "^5.8.1",
    "next": "^13.4.19",
    "next-auth": "^4.23.1",
    "prettier": "^3.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.45.1",
    "react-hot-toast": "^2.4.1",
    "react-number-format": "^5.3.0",
    "sass": "^1.66.1",
    "server-only": "^0.0.1",
    "typed-css-modules": "^0.7.2",
    "typescript": "5.2.2",
    "typescript-plugin-css-modules": "^5.0.1",
    "usehooks-ts": "^2.9.1",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "prisma": "^5.2.0",
    "typed-scss-modules": "^7.1.4"
  }
}
